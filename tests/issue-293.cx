package main

type event struct {
	index i32
}

var events []event

func foo(e *event) {
	test((*e).index, 33, "")
}

func main()(){
	var e event
	e.index = 33
	events = append(events, e)
	//var ee event = events[0]
	//foo(&ee) // <- this works
	foo(&events[0])
}
