	id str
	values [20]issue48type2
}

type issue48type2 struct {
	id str
	values [20]issue48type1
}

func main () {
	var a issue48type1
	var b issue48type2

	a.id = "type1"
	b.id = "type2"

	a.values[0] = b
	b.values[0] = a

	test(a.values[0].id, "type2", "")
	test(b.values[0].id, "type1", "")
}
