package main

func fooB(path str) {
	var ss []i32
	var INIT_HEAP_SIZE i32 = 32 * (1 << 20) // TODO : should be tested against real cx/config.go::INIT_HEAP_SIZE
	for i := 0; i < INIT_HEAP_SIZE; i++ {
		ss = append(ss, 1)
	}
}

func fooA(path str, filename str, s f32, r f32) {
	fooB(sprintf("%s/%s", path, filename))
}

func main()() {
	fooA("test", "A", 1.0, 2.0)
}

