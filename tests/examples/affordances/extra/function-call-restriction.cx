package main
import "aff"

func prgrmPredicate (prgrm aff.Program) (res bool) {
  	if prgrm.Caller.FnName == "main.bar" && prgrm.Caller.FnSize == 0 {
		res = true
	}
}

func foo () {
  	fltrs := #{
		filter(prgrmPredicate)
  	}
  	affs := aff.query(fltrs)
  	if len(affs) < 1 {
  		return
  	}
	
  	str.print(`I will only print if "main.bar" calls me`)
}

func bar () {
 	foo()
}

func main () {
  	foo()
  	bar()
}


